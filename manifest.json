{
    "manifest_version": 2,
  
    "name": "Curbing Misinformation - Covid-19",
    "description": "This extension will analyze misleading news",
    "version": "2.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "popup.html"
    },
    "permissions": [
     "activeTab"
     ]
  }